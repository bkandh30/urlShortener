{
  "name": "@bkandh30/common-url-shortener",
  "version": "1.0.6",
  "publishConfig": {
    "access": "public"
  },
  "description": "Common utilities and types for URL shortener",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/mjs/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf dist && tsc --module CommonJS --outDir dist/cjs && tsc --module ESNext --outDir dist/mjs",
    "prepack": "npm run build",
    "dev": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "devDependencies": {
    "@types/node": "^24.3.0",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "zod": "^4.1.4"
  }
}
